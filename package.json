{
  "name": "vuad",
  "version": "1.0.0",
  "description": "a Blog build with Vue, Typescript, Atomic Design & DDD",
  "main": "index.js",
  "keywords": [
    "vue",
    "typescript",
    "atomic-design",
    "ddd",
    "spa",
    "atomic-design-example",
    "ddd-example"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logustra/vuad"
  },
  "bugs": "https://github.com/logustra/vuad/issues",
  "author": "Faizal Andyka Putra",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config build/webpack.development --progress",
    "build": "yarn clean:dist && NODE_ENV=production webpack --config build/webpack.production --progress",
    "clean:dist": "rimraf dist",
    "start": "yarn build && node server"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^11.12.0",
    "@types/webpack-env": "^1.13.9",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.5",
    "commitizen": "^4.0.3",
    "core-js": "3.0.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "3.0.7",
    "husky": "^4.2.3",
    "node-sass": "^4.11.0",
    "nprogress": "^0.2.0",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.3.3",
    "typescript": "~3.1.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "iltorb": "^2.4.4",
    "node-zopfli-es": "^1.0.3",
    "shrink-ray-current": "^4.0.0"
  }
